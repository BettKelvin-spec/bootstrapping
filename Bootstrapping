    {
      "cell_type": "code",
      "source": [
        "# Define the storage location for the results as a Pandas DataFrame\n",
        "\n",
        "My_Simulation_Results = pd.DataFrame(columns = ['Color', 'Number'])\n",
        "\n",
        "# Define the colors\n",
        "\n",
        "colors =['Blue', 'Green']\n",
        "\n",
        "print(My_Simulation_Results)\n",
        "print(colors)"
      ],
      "metadata": {
